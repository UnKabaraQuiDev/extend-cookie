{
  "manifest_version": 3,
  "name": "7 Days SAML Cookies",
  "version": "1.0",
  "description": "Fix for shitty IAM SAML cookies",
  "permissions": [
    "cookies",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "saml-cookies@kbra.lu"
    }
  }
}
