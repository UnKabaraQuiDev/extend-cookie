{
  "manifest_version": 2,
  "name": "Extend cookies",
  "version": "1.1",
  "description": "Extend the expiration duration of cookies by domain.",
  "permissions": [
    "cookies",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
